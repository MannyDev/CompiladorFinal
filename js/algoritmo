 return c == ")"
}).length

cadena2.split("").each(function(c,i,a){

if(c=="{") pila.push({c:c,i:i});
if(c=="}") {
var inicio = pila.pop({c:c,i:i});
console.log(a.join("").substring(inicio.i+1,i))
